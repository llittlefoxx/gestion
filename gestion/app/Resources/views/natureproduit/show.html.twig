{% extends 'client.html.twig' %}
{% block stylesheets %}
    {{parent()}}
{% endblock %}

{% block body %}


    {% block coname %}    {{parent()}}  Nature de produit : {{ natureproduit.libelle }} {% endblock %}
    {{parent()}}


    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Libelle</th>
                <td>{{ natureproduit.libelle }}</td>
            </tr>
            <tr>
                <th>Datefinvalidite</th>
                <td>{% if natureproduit.datefinvalidite %}{{ natureproduit.datefinvalidite|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>

        </tbody>
    </table>

    <a href="{{ path('natureproduit_index') }}" class="btn btn-warning"  >Revenir a la list</a>
    <a href="{{ path('natureproduit_edit', { 'id': natureproduit.idnature }) }}" class="btn btn-info btn-md" >Modifier</a>
    <a href="{{ path('natureproduit_del', { 'id': natureproduit.idnature }) }}" class="btn btn-danger btn-md" >Supprimer</a>

{% endblock %}
