{% extends 'base.html.twig' %}

{% block body %}
    <h1>Facturefour</h1>

    <table>
        <tbody>
            <tr>
                <th>Numfactcni</th>
                <td>{{ facturefour.numfactcni }}</td>
            </tr>
            <tr>
                <th>Numfactfour</th>
                <td>{{ facturefour.numfactfour }}</td>
            </tr>
            <tr>
                <th>Datefour</th>
                <td>{% if facturefour.datefour %}{{ facturefour.datefour|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datecni</th>
                <td>{% if facturefour.datecni %}{{ facturefour.datecni|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Objetfact</th>
                <td>{{ facturefour.objetfact }}</td>
            </tr>
            <tr>
                <th>Idfactfour</th>
                <td>{{ facturefour.idfactfour }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('facturefour_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('facturefour_edit', { 'id': facturefour.idfactfour }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
