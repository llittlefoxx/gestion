<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Comptebancaire" table="comptebancaire">
    <id name="idcompte" type="integer" column="idcompte">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numrib" type="string" column="numRib" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datefinvalidite" type="datetime" column="dateFinValidite" nullable="true"/>
    <field name="proprietaire" type="string" column="proprietaire" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
